  backgroundLocked = !backgroundLocked;
   const iconName = backgroundLocked ? "lock" : "unlock";
  bgLockBtn.title = backgroundLocked ? "Unlock Background" : "Lock Background";
  bgLockBtn.innerHTML = `<i data-lucide="${iconName}" class="icon"></i>`;

  // Re-render Lucide icon
  if (window.lucide) lucide.createIcons();
});

        


        
        

